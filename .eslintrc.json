{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "camelcase": 0,
        "consistent-return": 0,
        "no-console": 0,
        "no-param-reassign": 0,
        "no-underscore-dangle": 0,
        "padded-blocks": 0,
        "radix": 0,
        "no-useless-catch": 0,
        "no-use-before-define": 0,
        "no-restricted-globals": 0,
        "no-shadow": 0,
        "no-fallthrough": 0,
        "linebreak-style": 0,
        "arrow-parens": ["error", "as-needed"]
    }
}
